{
  "name": "graphics-element",
  "version": "1.6.3",
  "type": "module",
  "description": "A custom HTML <graphics-element>",
  "main": "dist/graphics-element.js",
  "homepage": "https://github.com/pomax/custom-graphics-element#readme",
  "bugs": {
    "url": "https://github.com/pomax/custom-graphics-element/issues"
  },
  "scripts": {
    "build": "rm -rf dist && npm run esbuild && cp ./graphics-element/graphics-element.css dist && node finish-up.js",
    "esbuild": "esbuild graphics-element/graphics-element.js  --bundle --outfile=./dist/graphics-element.js --format=esm --minify --keep-names --tree-shaking=false",
    "test": "npm run build && run-p serve open",
    "serve": "serve --cors",
    "open": "open-cli \"http://localhost:3000\""
  },
  "author": "Pomax",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.19.10",
    "monaco-editor": "^0.45.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^8.0.0",
    "serve": "^14.2.1"
  }
}
